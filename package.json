{
  "name": "issue-forms-creator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:update": "ng test -u",
    "ct:run": "npx cypress run --component",
    "ct:open": "npx cypress open --component",
    "lint": "ng lint",
    "prettier:write": "prettier --write \"src/**/*.{js,json,css,scss,ts,html,component.html}\"",
    "prettier:check": "prettier \"src/**/*.{js,json,css,scss,ts,html,component.html}\" --list-different",
    "prepare": "husky install",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1402.9",
    "@angular/animations": "^14.2.7",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^14.2.7",
    "@angular/compiler": "^14.2.7",
    "@angular/core": "^14.2.7",
    "@angular/forms": "^14.2.7",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^14.2.7",
    "@angular/platform-browser-dynamic": "^14.2.7",
    "@angular/router": "^14.2.7",
    "@briebug/jest-schematic": "^4.0.0",
    "install": "^0.13.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.1.1",
    "npm": "^8.12.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "14.0.1",
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.7",
    "@angular/elements": "^14.2.7",
    "@babel/core": "^7.14.6",
    "@compodoc/compodoc": "^1.1.14",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/angular": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.12",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "29.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^4.0.7",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.27.0",
    "@webcomponents/custom-elements": "^1.5.1",
    "babel-loader": "^8.2.2",
    "cypress": "^10.10.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.4.0",
    "jest": "28.0.0",
    "jest-preset-angular": "^12.2.2",
    "karma-coverage": "~2.2.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.3.1",
    "typescript": "~4.6.4"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --cache --fix",
    "src/**/*.{js,json,css,scss,ts,html,component.html}": "prettier --write"
  }
}
